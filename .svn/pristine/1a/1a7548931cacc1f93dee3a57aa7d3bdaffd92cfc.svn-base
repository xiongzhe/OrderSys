#import <Foundation/Foundation.h>

/**
 * 优惠券实体
 **/
@interface CouponsInfo : NSObject

@property(nonatomic,assign) NSInteger couponsId; //id
@property(nonatomic,assign) NSInteger *typeId; //优惠券类型id
@property(nonatomic,retain) NSString *typeName; //优惠券类型名
@property(nonatomic,assign) NSInteger top; //满
@property(nonatomic,assign) NSInteger sub; //减
@property(nonatomic,retain) NSArray *dishTypes; //优惠券涉及菜品
@property(nonatomic,retain) NSString *dishTypeStr; //优惠券涉及菜品字符串

@end
